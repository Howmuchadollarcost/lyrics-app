<% data.forEach(song => { %>
    <div class="col s6 m6">
        <div class="card">
            <div class="card-image">
                <img id="image" src="<%= song.imgSrc %>">
                <span class="card-title"><%= song.artistName %></span>
            </div>
            <div class="card-content">
                <p><%= song.songName %></p>
            </div>
            <div class="card-action">
                <a class="card-link" href="/songs/<%= song._id %>">View</a>
            </div>
        </div>
    </div>
   <% });%>

